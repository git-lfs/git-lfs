{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Git LFS HTTPS Lock Verify API Response",
  "type": "object",

  "definitions": {
    "lock": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "locked_at": {
          "type": "string"
        },
        "owner": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          }
        }
      },
      "required": ["id", "path"]
    }
  },

  "properties": {
    "ours": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/lock"
      }
    },
    "theirs": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/lock"
      }
    },
    "next_cursor": {
      "type": "string"
    }
  },
  "required": ["ours", "theirs"]
}
